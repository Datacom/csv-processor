<h1>Listing field_mappings</h1>

<table>
  <thead>
    <tr>
      <th>Src field name</th>
      <th>Out field name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @field_mappings.each do |field_mapping| %>
      <tr>
        <td><%= field_mapping.src_field_name %></td>
        <td><%= field_mapping.out_field_name %></td>
        <td><%= link_to 'Show', field_mapping %></td>
        <td><%= link_to 'Edit', edit_field_mapping_path(field_mapping) %></td>
        <td><%= link_to 'Destroy', field_mapping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Field mapping', new_field_mapping_path %>
